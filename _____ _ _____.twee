:: StoryStylesheet [stylesheet]
tw-passage {
  color: yellow;
  background-color: black;
  font-size: 25px;
  text-align: left;
  padding-left: 0; 
}

tw-link {
  color: black;
  background-color: yellow;
}
tw-link:hover {
  color: #333;
  background-color: #ff0;
}
tw-link {
  color: black;
  background-color: yellow;
  transition: background-color 0.3s, color 0.3s; 
}

tw-link:hover {
  color: black; 
  background-color: red; 
}
tw-link {
  color: black;
  background-color: yellow;
  display: block;
  width: 55%;
  border: none;
  outline: none;
  padding: 10px;
  text-align: left;
  text-decoration: none;
  cursor: pointer;
  transition: background-color 0.3s;
}

tw-link:hover {
  background-color: #ccc;
}